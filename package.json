{
  "name": "blog-qwik",
  "description": "Doğan Öztürk's Personal Blog",
  "homepage": "https://github.com/doganozturk/blog-qwik#readme",
  "email": "doganozturk2005@gmail.com",
  "url": "https://doganozturk.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/doganozturk/blog-qwik.git"
  },
  "engines": {
    "node": ">=22.0.0 <23"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "build": "qwik build && npm run build.static",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build --ssr src/entry.ssr.tsx",
    "build.static": "vite build --config adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "qwik": "qwik",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.17.0",
    "@builder.io/qwik-city": "^1.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/eslint": "8.44.6",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "8.46.2",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.17.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.2.5",
    "node-fetch": "3.3.2",
    "prettier": "^3.6.2",
    "typescript": "~5.9.0",
    "vite": "5.4.19",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "date-fns": "^2.29.3"
  }
}
